{
	"name": "language-client-example",
	"description": "VSCode part of a language server",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"version": "0.0.1",
	"publisher": "vscode",
	"engines": {
		"vscode": "^1.6.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:html"
	],
	"main": "./out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"properties": {
				"ngIntelliSense.ignore": {
					"type": [
						"array",
						"null"
					],
					"default": [
						"node_modules",
						"test"
					]
				},
				"ngIntelliSense.componentGlob": {
					"type": [
						"string"
					],
					"default": "**/*Component.ts"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.3",
		"@types/node": "^6.0.46",
		"@types/decamelize": "0.0.30",
    	"@types/glob": "^5.0.30"
	},
	"dependencies": {
		"glob": "^7.1.1",
		"decamelize": "^1.2.0"
	}
}