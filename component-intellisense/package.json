{
  "name": "vscode-angular-components-intellisense",
  "description": "Angular 1.5 components intellisense",
  "author": "Microsoft Corporation",
  "license": "MIT",
  "version": "0.1.0",
  "publisher": "vscode",
  "engines": {
    "vscode": "^1.6.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "ngIntelliSense.ignore": {
          "type": [
            "array",
            "null"
          ],
          "default": [
            "node_modules",
            "test"
          ]
        },
        "ngIntelliSense.componentGlob": {
          "type": [
            "string"
          ],
          "default": "**/*Component.ts"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/decamelize": "0.0.30",
    "@types/glob": "^5.0.30",
    "@types/lodash": "^4.14.38",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.46",
    "typescript": "^2.0.3",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "decamelize": "^1.2.0",
    "glob": "^7.1.1",
    "lodash": "^4.16.6",
    "moment": "^2.15.2"
  }
}
